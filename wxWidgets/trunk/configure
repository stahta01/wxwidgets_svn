        STRIP=$host_alias-strip
    else
        { { echo "$as_me:$LINENO: error: $build_alias -> $host_alias cross compilation not supported yet." >&5
echo "$as_me: error: $build_alias -> $host_alias cross compilation not supported yet." >&2;}
   { (exit 1); exit 1; }; }
    fi
fi

