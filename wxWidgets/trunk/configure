    cat >>confdefs.h <<\_ACEOF
#define __LINUX__ 1
_ACEOF

    TMP=`uname -m`
    if test "x$TMP" = "xalpha"; then
      USE_ALPHA=1
      cat >>confdefs.h <<\_ACEOF
#define __ALPHA__ 1
_ACEOF

    fi
    DEFAULT_DEFAULT_wxUSE_GTK=1
  ;;
  *-*-gnu* )
    USE_GNU=1
    cat >>confdefs.h <<\_ACEOF
#define __GNU__ 1
_ACEOF

    TMP=`uname -m`
    if test "x$TMP" = "xalpha"; then
      USE_ALPHA=1
      cat >>confdefs.h <<\_ACEOF
#define __ALPHA__ 1
_ACEOF

    fi
    DEFAULT_DEFAULT_wxUSE_GTK=1
  ;;
  *-*-irix5* | *-*-irix6* )
    USE_SGI=1
    USE_SVR4=1
    cat >>confdefs.h <<\_ACEOF
#define __SGI__ 1
_ACEOF

    cat >>confdefs.h <<\_ACEOF
#define __SVR4__ 1
_ACEOF

    DEFAULT_DEFAULT_wxUSE_MOTIF=1
  ;;
  *-*-solaris2* )
