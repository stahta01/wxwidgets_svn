    DEFAULT_DEFAULT_wxUSE_MAC=1
  ;;

  *-*-beos* )
        USE_BEOS=1
    cat >>confdefs.h <<\_ACEOF
#define __BEOS__ 1
_ACEOF

  ;;

  *)
    { { echo "$as_me:$LINENO: error: unknown system type ${host}." >&5
echo "$as_me: error: unknown system type ${host}." >&2;}
   { (exit 1); exit 1; }; }
esac




