#define __SUN__ 1
_ACEOF

    cat >>confdefs.h <<\_ACEOF
#define __SOLARIS__ 1
_ACEOF

    cat >>confdefs.h <<\_ACEOF
#define __SVR4__ 1
_ACEOF

