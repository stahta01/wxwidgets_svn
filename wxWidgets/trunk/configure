#define __SUN__ 1
_ACEOF

    cat >>confdefs.h <<\_ACEOF
#define __SUNOS__ 1
_ACEOF

    cat >>confdefs.h <<\_ACEOF
#define __BSD__ 1
_ACEOF

    DEFAULT_DEFAULT_wxUSE_MOTIF=1
  ;;
  *-*-freebsd*)
