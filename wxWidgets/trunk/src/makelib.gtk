WXDIR = $(WXWIN)

include $(WXDIR)/src/gtk.env

all:    $(LIBTARGET)

$(LIBTARGET): $(OBJECTS)
	ar $(AROPTIONS) $@ $(OBJECTS)
	$(RANLIB) $@

clean:
	rm -f $(OBJECTS) $(LIBTARGET) core *.rsc *.res
