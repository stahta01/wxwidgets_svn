		$(XPMDIR)\crbuffri.obj\
		$(XPMDIR)\crdatfri.obj\
		$(XPMDIR)\create.obj\
		$(XPMDIR)\crifrbuf.obj\
		$(XPMDIR)\crifrdat.obj\
		$(XPMDIR)\dataxpm.obj\
		$(XPMDIR)\imagexpm.obj\
		$(XPMDIR)\info.obj\
