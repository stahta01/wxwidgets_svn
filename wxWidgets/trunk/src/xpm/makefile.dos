		$(XPMDIR)\simx.obj\
		$(XPMDIR)\wrffrdat.obj\
		$(XPMDIR)\wrffri.obj

all:    $(LIBTARGET)

$(LIBTARGET):      $(OBJECTS)
	-erase $(LIBTARGET)
	lib /PAGESIZE:128 @<<
$(LIBTARGET)
y
$(OBJECTS)
nul
;
<<

